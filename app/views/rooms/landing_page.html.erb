<div class="col-md-6">
  <h1>Welcome Back!</h1>
    <div class="well well-lg"
      <h3>What would you like to do?</h3>


      <p>
          <% if user_signed_in? %>
              <a><%= link_to "View all rooms", rooms_path, class: "btn btn-info"  %></a>
              <a><%= link_to "View Your Bookings", reservations_path, class: "btn btn-info"  %></a>
              <a><%= link_to "Edit Your Profile", edit_user_registration_path, class: "btn btn-default" %></a>
              <a><%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-default"  %></a>
          <% else %>
              <p>please sign up</p>
          <% end %>
      </p>
    </div>
</div>
